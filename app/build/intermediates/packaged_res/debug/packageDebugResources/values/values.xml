<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="accent_color">#DEDEDE</color>
    <color name="ic_launcher_background">#1A1A1A</color>
    <color name="light_accent">#ABABAB</color>
    <color name="text_color">#3C3C3C</color>
    <color name="white_background">#F6F6F6</color>
    <style name="Theme.ReminderApp" parent="Theme.Material3.DayNight.NoActionBar">
        
        <item name="android:statusBarColor">@color/white_background</item>
        <item name="android:navigationBarColor">@color/white_background</item>
        


    </style>
    <style name="Theme.ReminderApp.Splash" parent="Theme.SplashScreen">
        <item name="windowSplashScreenBackground">@color/white_background</item>
        <item name="windowSplashScreenAnimatedIcon">@drawable/heyreminder_splash_light</item>
        <item name="postSplashScreenTheme">@style/Theme.ReminderApp</item>
    </style>
</resources>